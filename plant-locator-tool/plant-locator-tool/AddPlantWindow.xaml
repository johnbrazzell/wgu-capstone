<Window x:Class="plant_locator_tool.AddPlantWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:plant_locator_tool"
        mc:Ignorable="d"
        Title="AddPlantWindow" Height="450" Width="800">
    <StackPanel>
        <Label Content="Plant/Warehouse Name:"/>
        <Label Content="Produces/Sourced By"/>
        <Label Content="Street"/>
        <Label Content="City"/>
        <Label Content="State"/>
        <Label Content="Zip"/>
        <Label Content="Add"/>
        <Label Content="Cancel"/>

    </StackPanel>
</Window>
